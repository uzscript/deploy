{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
